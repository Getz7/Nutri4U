@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/css/NutriMenu.css" type="text/css" rel="stylesheet" />

</head>
<body>
    <header class="header">
        <div class="logo">
            <div>
                <img src="../Resources/LogoCompletoBlanco.png" alt="">
            </div>
        </div>

        <nav class="navbar">
           
            <a href="@Url.Action("ViewFood","Nutri")">Lista de Productos</a>
            <a href="@Url.Action("CreateFood","Nutri")">Registrar Producto</a>
            <a href="@Url.Action("Perfil","Nutri")">Mi Perfil</a>
            <a id="logout">Cerrar Sesión</a>
         
        </nav>
    </header>
     <div class="main-container">
            <div class="slideshow-container">

                <!-- Full-width images with number and caption text -->
                <div class="mySlides fade">
                    <img src="../Resources/Carrusel1.jpg" style="width:100%">
               
                </div>

                <div class="mySlides fade">
                    <img src="../Resources/Carrusel4.jpg" style="width:100%">
                
                </div>


                <!-- Next and previous buttons -->
                <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
                <a class="next" onclick="plusSlides(1)">&#10095;</a>
            </div>

        <!-- The dots/circles -->
            <div style="text-align:center">
                <span class="dot" onclick="currentSlide(1)"></span>
                <span class="dot" onclick="currentSlide(2)"></span>
            </div>
        <h1>Registrar Un Producto</h1>
        <div class="add-container">

            <a href="@Url.Action("CreateFood","Nutri")">
                <i class="fa-solid fa-circle-plus"></i>
            </a>

            
        </div>
     </div>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/swal/dist/sweetalert2.all.min.js"></script>
    <script src="~/js/MenuNutri.js" asp-append-version="true"></script>
    
    <script src="https://kit.fontawesome.com/79ebbbb4e2.js" crossorigin="anonymous"></script>
</body>
</html>