@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/Login.css">
</head>
<body>
    <div class="container">
        <div class="left-panel">
            <img src="../Resources/img4.jpg" alt="">
        </div>
        <div class="right-panel">
            <img src="../Resources/NegroNoBG.png" alt="">
            <form action="#">
                <div class="input-container">
                    <label for="email">Correo:</label>
                    <input class="required" type="email" id="txt-email" name="email" required>
                </div>
                <div class="input-container">
                    <label for="password">Contraseña:</label>
                    <input class="required" type="password" id="txt-password" name="password" required>
                </div>
                <button id="btnLogin" type="submit">Iniciar Sesión</button>
            </form>
            <a href="@Url.Action("CreateUser", "LandingPage")">Crear cuenta</a>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/swal/dist/sweetalert2.all.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/Login.js"></script>
</body>
</html>